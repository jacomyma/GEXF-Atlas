math_max:
{math_max( 2, 4 )} {math_max( 2, 4, 3 )}

math_min:
{math_min( 2, 4 )} {math_min( 2, 4, 3 )}

math_abs:
{math_abs( -2 )} {math_abs( 4 )}

math_ceil:
{math_ceil( 2.2 )} {math_ceil( 2.9 )}

math_floor:
{math_floor( 2.2 )} {math_floor( 2.9 )}

math_round:
{math_round( 2.2 )} {math_round( 2.9 )}

math_sqrt:
{math_sqrt( 9 )} {math_sqrt( 4 )}

math_exp:
{math_round( math_exp( 5.7 ) )}

math_pow:
{math_pow( 2, 3 )}

math_log:
{math_log( 8, 2 )}

math_log10:
{math_log10( 100 )}

math_float_mod:
{math_float_mod( 5.7, 1.3 )}

