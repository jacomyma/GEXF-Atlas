{var $a = 1..3}
{$a[0]}{$a[1]}{$a[2]}
{$a = 5..2}
{$a[0]}{$a[1]}{$a[2]}{$a[3]}
{$a = -2..2}
{$a[0]}{$a[1]}{$a[2]}{$a[3]}{$a[4]}
{$a = 2..-2}
{$a[0]}{$a[1]}{$a[2]}{$a[3]}{$a[4]}

{$a = 1+0 .. 2*2}
{$a[0]}{$a[1]}{$a[2]}{$a[3]}

{$a = 'a'..'d'}
{$a[0]}{$a[1]}{$a[2]}{$a[3]}

{var $val1 = 1, $val2 = 4}
{$a = $val1 .. $val2}
{$a[0]}{$a[1]}{$a[2]}{$a[3]}
