{var $a = 0, $b = 0}
{foreach 1..4 as $a}
  {$a}
  {delimiter}
    {foreach 1..10 as $b}
      :{$b}:
    {/foreach}
  {/delimiter}
{/foreach}
============
{foreach 1..4 as $a}
  {$a}
  {delimiter}
    {$b = 1}
    {while $b <= 10}
      :{$b}:
      {$b++}
    {/while}
  {/delimiter}
{/foreach}
