{* file: nested_with_delimiter_0025.in *}
{var $foo = array( 0, 1, 3 ), $foo2 = array( '', 'foo' ), $bar = 0, $bar2 = 0, $num = 0, $num2 = 0, $i = 0, $j = 0}
\{0025
{foreach 1..1 as $blackhole}
    {$i = 0}
    {foreach $foo as $bar}
        {switch $bar}
        {case 1}
            {$i}:[switch]1[/switch]
        {/case}
        {case 2}
            {$i}:[switch]2[/switch]
        {/case}
        {default}
            {$i}:[switch]default({$bar})[/switch]
        {/default}
        {/switch}
        {$j = 0}
        {if $foo2[1]}
            {delimiter}
                {$i}:{$j}:~~delim~~
            {/delimiter}
            {delimiter}
                {$i}:{$j}:====delim====
            {/delimiter}
            {delimiter modulo 2}
                {$i}:{$j}:____delim____
            {/delimiter}
        {/if}
    {++$i}
    {/foreach}
{/foreach}
\}
